<ion-view id="tabsMaster">
    <ion-content scroll="false">
        <ion-slide-box show-pager="false" ion-slide-tabs>
            <ion-slide ion-slide-tab-label="profile">
                <ng-include src="'profile/profile.html'"></ng-include>
            </ion-slide>
            <ion-slide ion-slide-tab-label="home">
                <ng-include src="'recipesList/recipesList.html'"></ng-include>
            </ion-slide>
            <ion-slide ion-slide-tab-label="list">
                <ng-include src="'shoppingList/listHome/listHome.html'"></ng-include>
            </ion-slide>
        </ion-slide-box>
    </ion-content>
</ion-view>